// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  username   String    @id
  password   String
  name       String
  token      String?
  nik        String?
  user_type  String
  last_login DateTime?

  @@map("users")
}
